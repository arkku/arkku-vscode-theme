  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#bbbbbb"
      }
    },
    {
      "name": "Quote",
      "scope": "quote",
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#884466"
      }
    },
    {
      "name": "Single-quoted string",
      "scope": [
        "string.quoted.single"
      ],
      "settings": {
        "foreground": "#dd6600"
      }
    },
    {
      "name": "JSON String",
      "scope": [
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "name": "Unquoted String",
      "scope": [
        "string.unquoted"
      ],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "name": "Preprocessor",
      "scope": [
        "meta.preprocessor",
        "meta.preprocessor keyword",
        "meta.preprocessor.include string.quoted",
        "meta.embedded.frontmatter"
      ],
      "settings": {
        "foreground": "#006600"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp",
        "string.regexp keyword.other",
        "regexp-operator",
        "string.regexp.characterclass punctuation.definition.string.begin",
        "string.regexp.characterclass punctuation.definition.str"
      ],
      "settings": {
        "foreground": "#dd9900"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.character.numeric",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#0099AA"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "support.constant",
        "support.constant.json",
        "support.constant.math",
        "constant.language"
      ],
      "settings": {
        "foreground": "#0066cc"
      }
    },
    {
      "name": "User-defined Constant",
      "scope": [
        "constant.character",
        "constant.other",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#0041A3"
      }
    },
    {
      "name": "Comma",
      "scope": "meta.function punctuation.separator.comma",
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.parameter",
        "support.other.variable"
      ],
      "settings": {
        "foreground": "#003366"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "bitwise-operator",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": ""
      }
    },
    {
      "name": "Case",
      "scope": [
        "keyword.control.case",
        "meta.conditional keyword.control"
      ],
      "settings": {
        "foreground": "#0066cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "meta.var.expr",
        "meta.class meta.method.declaration meta.var.expr storage.type.js",
        "storage.type.property.js",
        "storage.type.property.ts",
        "storage.type.property.tsx"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Storage Type",
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type.function.arrow.js",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Class Name",
      "scope": [
        "entity.name.class",
        "support.type",
        "support.class",
        "meta.class entity.name.type.class"
      ],
      "settings": {
        "foreground": "#5C2699"
      }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function Name",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.tag.swift"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Meta Tag",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#63a35c"
      }
    },
    {
      "name": "Custom Tag",
      "scope": "entity.name.tag.custom",
      "settings": {
        "foreground": "#5ca388"
      }
    },
    {
      "name": "Attribute",
      "scope": [
        "entity.other.attribute-name",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#795da3"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#e22e28"
      }
    },
    {
      "name": "Separator",
      "scope": "meta.separator",
      "settings": {
        "foreground": "#bbbbbb"
      }
    },
    {
      "name": "Heading",
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Code Block",
      "scope": "markup.raw.block",
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "name": "Link and Image",
      "scope": [
        "meta.link",
        "meta.image"
      ],
      "settings": {
        "foreground": "#4183c4"
      }
    },
    {
      "name": "Link URL",
      "scope": [
        "markup.underline.link",
        "constant.other.reference"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "List",
      "scope": "markup.list",
      "settings": {
        "foreground": "#444444"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "bold",
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": [
        "italic",
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Bold Italic",
      "scope": [
        "bold italic",
        "markup.bold markup.italic",
        "italic bold",
        "markup.italic markup.bold"
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    }
  ],
  "type": "light",
  "colorSpaceName": "sRGB",
  "semanticClass": "theme.light.arkku",
  "author": "Kimmo Kulovesi"
}

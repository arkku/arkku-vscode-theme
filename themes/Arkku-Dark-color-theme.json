  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "name": "Quote",
      "scope": "quote",
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#cc99cc"
      }
    },
    {
      "name": "Single-quoted string",
      "scope": [
        "string.quoted.single"
      ],
      "settings": {
        "foreground": "#cc88bb"
      }
    },
    {
      "name": "JSON String",
      "scope": [
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Unquoted String",
      "scope": [
        "string.unquoted"
      ],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Preprocessor",
      "scope": [
        "meta.preprocessor",
        "meta.preprocessor keyword",
        "meta.preprocessor.include string.quoted",
        "meta.embedded.frontmatter"
      ],
      "settings": {
        "foreground": "#00aa33"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp",
        "string.regexp keyword.other",
        "regexp-operator",
        "string.regexp.characterclass punctuation.definition.string.begin",
        "string.regexp.characterclass punctuation.definition.str"
      ],
      "settings": {
        "foreground": "#dd9900"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "support.constant",
        "support.constant.json",
        "support.constant.math",
        "constant.language"
      ],
      "settings": {
        "foreground": "#00aadd"
      }
    },
    {
      "name": "User-defined Constant",
      "scope": [
        "constant.character",
        "constant.other",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#00bbdd"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.character.numeric",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#00bbdd"
      }
    },
    {
      "name": "Comma",
      "scope": "meta.function punctuation.separator.comma",
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.parameter",
        "support.other.variable"
      ],
      "settings": {
        "foreground": "#33aacc"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#f0f0ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "bitwise-operator",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": ""
      }
    },
    {
      "name": "Case",
      "scope": [
        "keyword.control.case",
        "meta.conditional keyword.control"
      ],
      "settings": {
        "foreground": "#33cc99",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "meta.var.expr",
        "meta.class meta.method.declaration meta.var.expr storage.type.js",
        "storage.type.property.js",
        "storage.type.property.ts",
        "storage.type.property.tsx"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Storage Type",
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#f0f0f0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type.function.arrow.js",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Class Name",
      "scope": [
        "entity.name.class",
        "support.type",
        "support.class",
        "meta.class entity.name.type.class"
      ],
      "settings": {
        "foreground": "#aaaabb"
      }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function Name",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.tag.swift"
      ],
      "settings": {
        "foreground": "#f0f0f0"
      }
    },
    {
      "name": "Meta Tag",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#99ccaa"
      }
    },
    {
      "name": "Custom Tag",
      "scope": "entity.name.tag.custom",
      "settings": {
        "foreground": "#aabbbb"
      }
    },
    {
      "name": "Attribute",
      "scope": [
        "entity.other.attribute-name",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#bbaabb"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#ee1133"
      }
    },
    {
      "name": "Separator",
      "scope": "meta.separator",
      "settings": {
        "foreground": "#444444"
      }
    },
    {
      "name": "Heading",
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Code Block",
      "scope": "markup.raw.block",
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Link and Image",
      "scope": [
        "meta.link",
        "meta.image"
      ],
      "settings": {
        "foreground": "#99bbff"
      }
    },
    {
      "name": "Link URL",
      "scope": [
        "markup.underline.link",
        "constant.other.reference"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "List",
      "scope": "markup.list",
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "bold",
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": [
        "italic",
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Bold Italic",
      "scope": [
        "bold italic",
        "markup.bold markup.italic",
        "italic bold",
        "markup.italic markup.bold"
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    }
  ],
  "type": "dark",
  "colorSpaceName": "sRGB",
  "semanticClass": "theme.dark.arkku",
  "author": "Kimmo Kulovesi"
}
